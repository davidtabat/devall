<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Change Account Company</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Devall\Tabatadze\Block\CompanyList" name="company_info_block" as="companyinfos" template="Devall_Tabatadze::companyinfo.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
